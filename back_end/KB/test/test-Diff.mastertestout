DiffMachine := rec(
           isFSA := true,
        alphabet := rec(
                type := "product",
                size := 24,
               arity := 2,
             padding := _,
                base := rec(
                    type := "identifiers",
                    size := 4,
                  format := "dense",
                   names := [a,a^-1,b,b^-1]
                   )
               ),
          states := rec(
                type := "words",
                size := 81,
            alphabet := [a,a^-1,b,b^-1],
              format := "sparse",
               names := [
                [1,IdWord],
                [2,a^-1*b^-1*b^-1*b^-1*a^-1*b^-1],
                [3,a^-1],
                [4,a*b*b*b*a*b],
